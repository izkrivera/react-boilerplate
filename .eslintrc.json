{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true,
    "commonjs": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:jsx-a11y/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": { "jsx": true }
  },
  "plugins": ["react", "jsx-a11y"],
  "rules": {},
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  }
}
